        <telerik:RadCodeBlock runat="server" ID="mainCodeBlock">
            <script type="text/javascript">
                var EPermissionOption = { None: 0, Create: 1, Edit: 2, Delete: 4, View: 8, Print: 16, Export: 32 };
                var EPermission = {
                    1: "账套管理",
                    2: "银行账号管理",
                    3: "供应商管理",
                    4: "商业单位管理",
                    5: "客户管理",
                    6: "仓库管理",
                    7: "配送公司管理",
                    8: "大包客户协议管理",
                    9: "商业客户流向管理",
                    10: "部门销售计划及提成策略",
                    11: "物流公司管理",
                    12: "货品管理",
                    13: "货品定价管理",
                    14: "库存货品信息",
                    15: "部门管理",
                    16: "员工管理",
                    17: "权限管理",
                    18: "报销类型管理",
                    19: "物流费用管理",
                    20: "担保收款",
                    21: "数据导入",
                    22: "部门月提成",
                    23: "采购订单报表",
                    24: "采购付款明细表",
                    25: "销售订单报表",
                    26: "出库明细表",
                    27: "入库明细表",
                    28: "采购计划表",
                    29: "库存汇总表",
                    30: "配送公司流向结算表",
                    31: "大包客户季度考核表",
                    32: "配送公司库存核对表",
                    33: "发票管理",
                    34: "大包客户结算表",
                    35: "供应商任务统计表",
                    36: "现金管理",
                    37: "借款管理",
                    38: "固定资产管理",
                    39: "担保收款管理",
                    40: "首页-库存提醒",
                    41: "首页-保证金提醒",
                    42: "首页-货品资料过期提醒",
                    43: "首页-客户资料过期提醒",
                    44: "首页-供应商资料过期提醒",
                    45: "首页-货品过期提醒",
                    46: "首页-借款到期提醒",
                    47: "首页-担保收款到期提醒"
                };
                var EWorkflow = {
                    1: "采购订单",
                    2: "采购入库单",
                    3: "大包申请单",
                    4: "大包配送订单",
                    5: "大包出库单",
                    6: "客户订单",
                    7: "客户订单出库单",
                    8: "供应商返款",
                    9: "客户返款",
                    10: "厂家经理返款",
                    11: "供应商任务返款",
                    12: "客户任务奖励返款",
                    13: "大包客户提成结算",
                    14: "供应商发票结算",
                    15: "客户发票结算",
                    16: "大包收款",
                    17: "挂靠发票结算",
                    18: "供应商保证金申请",
                    19: "客户保证金",
                    20: "客户保证金退回申请",
                    23: "工资结算管理"
                };
