            <script type="text/javascript">

                function getPermissionID(name) {

                    var json = EPermission;
                    var id = null;
                    $.each(json, function (i) {
                        var key = i;
                        var value = json[i];
                        if (value.toString() == name.toString()) {
                            id = key;
                        }
                    });
                    return id;
                }
                function getWorkflowPermissionID(name) {

                    var json = EWorkflow;
                    var id = null;
                    $.each(json, function (i) {
                        var key = i;
                        var value = json[i];
                        if (value.toString() == name.toString()) {
                            id = key;
                        }
                    });
                    return id;
                }
                function getPermissionOptionID(name) {
                    if (name == "None")
                        return EPermissionOption.None;
                    if (name == "Create")
                        return EPermissionOption.Create;
                    if (name == "Edit")
                        return EPermissionOption.Edit;
                    if (name == "Delete")
                        return EPermissionOption.Delete;
                    if (name == "View")
                        return EPermissionOption.View;
                    if (name == "Print")
                        return EPermissionOption.Print;
                    if (name == "Export")
                        return EPermissionOption.Export;
                }
                function checkPermission(id) {
                    var permissions = $("#hfPermissions").val();
                    var jsonPermissions = $.parseJSON(permissions);
                    var hasPermission = false;
                    $.each(jsonPermissions, function (i) {
                        var key = i;
                        var value = jsonPermissions[i];
                        if (key == id) {
                            hasPermission = true;
                        }
                    });
                    return hasPermission;
                }
                function checkWorkflowPermission(id) {
                    var permissions = $("#hfWorkFlowIDs").val();
                    var jsonPermissions = $.parseJSON(permissions);
                    var hasPermission = false;
                    $.each(jsonPermissions, function (i) {
                        var key = i;
                        var value = jsonPermissions[i];
                        if (value == id) {
                            hasPermission = true;
                        }
                    });
                    return hasPermission;
                }
                function getPermissionValue(id) {
                    var permissions = $("#hfPermissions").val();
                    var jsonPermissions = $.parseJSON(permissions);
                    var permissionValue = null;
                    $.each(jsonPermissions, function (i) {
                        var key = i;
                        var value = jsonPermissions[i];
                        if (key == id) {
                            permissionValue = value;
                        }
                    });
                    return permissionValue;
                }
                function checkMenu() {
                    var isSystemAdmin = $("#hfIsSystemAdmin").val();
                    if (isSystemAdmin == "True")
                        return;
                    var permissions = $("#hfPermissions").val();
                    console.log(permissions);
                    var jsonPermissions = $.parseJSON(permissions);
                    $.each(jsonPermissions, function (i) {
                        var key = i;
                        var value = jsonPermissions[i];

                        var Create = ((value & EPermissionOption.Create) == EPermissionOption.Create);
                        var Edit = ((value & EPermissionOption.Edit) == EPermissionOption.Edit);
                        var Delete = ((value & EPermissionOption.Delete) == EPermissionOption.Delete);
                        var View = ((value & EPermissionOption.View) == EPermissionOption.View);
                        var Print = ((value & EPermissionOption.Print) == EPermissionOption.Print);
                        var Export = ((value & EPermissionOption.Export) == EPermissionOption.Export);
                        //(((int)type & (int)value) == (int)value);
                        //alert(key + ":" + value + " create:" + Create + " Edit:" + Edit + " Delete：" + Delete + " View:" + View + " Print:" + Print + "Export:" + Export);
                    });

                    $("div#mws-navigation ul li[data-level='2']").each(function (i, e) {
                        var permissionName = $(this).data("permissionname");
                        var permissionOptionName = $(this).data("permissionoptionname");
                        if (permissionName && permissionOptionName) {
                            var permission = getPermissionID(permissionName);
                            var permissionOption = getPermissionOptionID(permissionOptionName);
                            //console.log("permission:" + permission + " permissionOption:" + permissionOption);
                            if (permission && permissionOption) {
                                console.log("permission:" + permission + " permissionName:" + permissionName + " permissionoption:" + permissionOption);
                                var hasPermission = checkPermission(permission);
                                //console.log("hasPermission:" + hasPermission);
                                if (hasPermission) {
                                    var permissionValue = getPermissionValue(permission);

                                    var hasPermissionOption = ((permissionValue & permissionOption) == permissionOption);
                                    if (hasPermissionOption)
                                    { }
                                    else {
                                        $(this).remove();
                                    }
                                }
                                else {
                                    $(this).remove();
                                }
                            }
                        }
                        var permissionWorkflowName = $(this).data("permissionworkflowname");
                        if (permissionWorkflowName) {
                            var workflowPermissionID = getWorkflowPermissionID(permissionWorkflowName);
                            if (checkWorkflowPermission(workflowPermissionID))
                            { }
                            else {
                                $(this).remove();
                            }
                        }
                    });
                    $("div#mws-navigation ul li[data-level='1']").each(function (i, e) {
                        var permissionnames = $(this).data("permissionnames");
                        var permissionoptionnames = $(this).data("permissionoptionnames");
                        if (permissionnames && permissionoptionnames) {
                            var hasPermissionLi = false;
                            var permissionnamesArray = permissionnames.split(";");
                            var permissionoptionnamesArray = permissionoptionnames.split(";");
                            for (var i = 0; i < permissionnamesArray.length; i++) {
                                var permission = getPermissionID(permissionnamesArray[i]);
                                var permissionOption = getPermissionOptionID(permissionoptionnamesArray[i]);
                                //console.log("permission:" + permission + " permissionOption:" + permissionOption);
                                if (permission && permissionOption) {
                                    var hasPermission = checkPermission(permission);
                                    //console.log("hasPermission:" + hasPermission);
                                    if (hasPermission) {
                                        var permissionValue = getPermissionValue(permission);
                                        var hasPermissionOption = ((permissionValue & permissionOption) == permissionOption);
                                        if (hasPermissionOption)
                                        {
                                            hasPermissionLi = true;
                                        }
                                    }
                                }
                            }
                            if (!hasPermissionLi)
                            {
                                $(this).remove();
                            }
                        }
                    });
                    $("div#mws-navigation ul li[data-level='1']").each(function (i, e) {
                        if ($(this).text() != "首页") {
                            var itemCount = $(this).find("ul li[data-level='2']").length;
                            if (itemCount == 0) {
                                $(this).remove();
                            }
                        }
                    });
                }
                var loadingPanel;//加载遮罩层
                var menuItemID;

                $(document).ready(function () {

                    checkMenu();

                    //BrowserDetect.init();

                    menuItemID = "<%=this.MenuItemID %>";

                    var curMenu = $("div#mws-navigation ul li[data-menuitemid='" + menuItemID + "']");

                    if (curMenu) {

                        //顶级菜单项
                        if (curMenu.attr("data-level") == "1") {

                            if (curMenu.next("ul").size() === 0) {
                                curMenu.addClass("active");
                            }

                        }//子菜单项
                        else {

                            curMenu.addClass("active-sub-nav");;

                            var curRootMenu = curMenu.parent().parent();

                            $("div#mws-navigation ul li[data-level='1']").each(function (i, e) {
                                $(this).removeClass("active");
                            });

                            curRootMenu.addClass("active");

                            $("div#mws-navigation ul li[data-level='1'] ul").each(function (index, e) {
                                $(this).hide();
                            });

                            var ulChilds = curRootMenu.find("ul");

                            if (ulChilds.size() !== 0) {
                                ulChilds.show();
                            }
                        }
                    }

                    loadingPanel = $find("<%= radAjaxLoadingPanel.ClientID %>");

                });

                //扩展jQuery，增加getRootPath函数
                (function ($) {
                    $.extend({
                        getRootPath: function () {
                            var rootPath = "<%= this.BaseUrl %>";
                            return rootPath;
                        }
                    })
                })(jQuery);

                function redirectToPage(url) {
                    //debugger;
                    try {
                        $.showLoading();

                        var isNeedHideLoading = "<%= this.IsNeedAutoHideLoading%>";

                        if (isNeedHideLoading.toLowerCase() === "true") {
                            var loadingTimeout = window.setTimeout(function (e) {
                                $.hideLoading();
                                window.clearTimeout(loadingTimeout);
                            }, 200);
                        }

                        if (url.indexOf("http:") >= 0 || url.indexOf("https:") >= 0)
                            window.location.href = url;
                        else
                            window.location.href = $.getRootPath() + url;
                    }
                    catch (e) {
                        //debugger;
                        $.hideLoading();
                    }
                }

                function onConfirmDelete() {
                    if (confirm("确认删除该条数据吗？")) {
                        return true;
                    }
                    return false;
                }

                function OnClientBeforeShow(sender, eventArgs) {
                    sender.center();
                }

                function onRowMouseOver(sender, eventArgs) {
                    var dataItem = eventArgs.get_gridDataItem();
                    if (dataItem) {
                        dataItem.addCssClass("grid-over-row");

                        var curRow = $(dataItem.get_element());

                        if (curRow.hasClass("rgAltRow"))
                            curRow.css({ "background-color": "" });
                    }
                }

                function onRowMouseOut(sender, eventArgs) {

                    var dataItem = eventArgs.get_gridDataItem();
                    if (dataItem) {
                        dataItem.removeCssClass("grid-over-row");

                        var curRow = $(dataItem.get_element());

                        if (curRow.hasClass("rgAltRow"))
                            curRow.css({ "background-color": "#f2f2f2" });
                    }
                }

            </script>

            <asp:ContentPlaceHolder ID="scriptContent" runat="server">
            </asp:ContentPlaceHolder>
        </telerik:RadCodeBlock>

